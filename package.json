{
	"name": "dark-reader-theme",
	"version": "1.0.0",
	"displayName": "Dark Reader Theme",
	"homepage": "https://github.com/ashrafhadden/dark-reader-theme",
	"description": "Dracula theme + Dark Reader background color",
	"publisher": "ashrafhadden",
	"license": "MIT",
	"scripts": {
		"build": "node ./scripts/build.js",
		"test": "node ./scripts/dev.js",
		"lint": "node ./scripts/lint.js",
		"start": "vsce package && code --install-extension ${npm_package_name}-${npm_package_version}.vsix",
		"vscode:prepublish": "npm update && npm run build"
	},
	"maintainers": [
		"Ashraf Hadden <ashrafhadden@gmail.com>"
	],
	"contributors": [
		"Zeno Rocha",
		"Derek P Sifford",
		"Krzysztof Wisznarewski",
		"Daniel Ramos",
		"Eric Jackson",
		"Elliott Pogue",
		"Ajit Singh",
		"Teddy Bradford"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ashrafhadden/dark-reader-theme"
	},
	"bugs": {
		"url": "https://github.com/ashrafhadden/dark-reader-theme/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dracula",
		"dark",
		"theme",
		"color-theme",
		"dark-reader"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#3c4557",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Dark Reader",
				"uiTheme": "vs-dark",
				"path": "./theme/dark-reader-theme.json"
			},
			{
				"label": "Dark Reader (Brightness +50)",
				"uiTheme": "vs-dark",
				"path": "./theme/dark-reader-theme-b50.json"
			},
			{
				"label": "Dark Reader (Brightness +50 contrast +25)",
				"uiTheme": "vs-dark",
				"path": "./theme/dark-reader-theme-b50-c25.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^3.13.1",
		"prettier": "^1.17.0"
	}
}
