{
  "name": "dracula-flat",
  "version": "1.1.0",
  "description": "Dracula theme with a haircut ✂️",
  "keywords": [
    "dracula",
    "flat-design",
    "vscode-theme"
  ],
  "homepage": "https://github.com/ashrafhadden/dracula-flat",
  "bugs": {
    "url": "https://github.com/ashrafhadden/dracula-flat/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashrafhadden/dracula-flat"
  },
  "license": "MIT",
  "author": {
    "name": "Ashraf Hadden",
    "email": "ashrafhadden@gmail.com",
    "url": "https://github.com/ashrafhadden"
  },
  "contributors": [
    "Ashraf Hadden",
    "Zeno Rocha",
    "Derek P Sifford",
    "Krzysztof Wisznarewski",
    "Daniel Ramos",
    "Eric Jackson",
    "Elliott Pogue",
    "Ajit Singh",
    "Teddy Bradford"
  ],
  "scripts": {
    "build": "node ./scripts/build.js",
    "lint": "node ./scripts/lint.js",
    "start": "vsce package && code --install-extension ${npm_package_name}-${npm_package_version}.vsix",
    "test": "node ./scripts/test.js",
    "vscode:prepublish": "npm update && npm run build"
  },
  "config": {
    "light": "Dracula Flat Light",
    "light-darker": "Dracula Flat Light Darker",
    "standard": "Dracula Flat"
  },
  "devDependencies": {
    "chroma-js": "^2.0.3",
    "js-yaml": "^3.13.1",
    "prettier": "^1.17.0"
  },
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dracula Flat",
        "uiTheme": "vs-dark",
        "path": "./themes/dracula-flat.json"
      },
      {
        "label": "Dracula Flat Light",
        "uiTheme": "vs",
        "path": "./themes/dracula-flat-light.json"
      },
      {
        "label": "Dracula Flat Light Darker",
        "uiTheme": "vs",
        "path": "./themes/dracula-flat-light-darker.json"
      }
    ]
  },
  "displayName": "Dracula Flat",
  "galleryBanner": {
    "color": "#282A36",
    "theme": "dark"
  },
  "icon": "icon.png",
  "maintainers": [
    "Ashraf Hadden <ashrafhadden@gmail.com>"
  ],
  "publisher": "ashrafhadden"
}
